{
  "version": 2,
  "builds": [
    {
      "src": "course-compass-backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "course-compass-backend/wsgi.application"
    }
  ],
  "buildCommand": "./build.sh"
}