{
    "version": 2,
    "functions": {
      "api/index.py": {
        "runtime": "@vercel/python"
      }
    },
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/course-compass-backend/staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.py"
      }
    ]
  }
  